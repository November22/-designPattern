package impl;

import abs.Target;

/**
 * 对Adaptee的接口与Target接口进行适配
 * @author sen.huang
 *
 */
public class Adapter implements Target{

	private Adaptee adaptee;
	
	@Override
	public void adapteeMethod() {
		adaptee.adapteeMethod();
		
	}

	@Override
	public void adapterMethod() {
		// TODO Auto-generated method stub
		
	}

}
